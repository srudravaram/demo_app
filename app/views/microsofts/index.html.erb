<h1>Listing microsofts</h1>

<table>
  <tr>
    <th>Content</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @microsofts.each do |microsoft| %>
  <tr>
    <td><%= microsoft.content %></td>
    <td><%= microsoft.user_id %></td>
    <td><%= link_to 'Show', microsoft %></td>
    <td><%= link_to 'Edit', edit_microsoft_path(microsoft) %></td>
    <td><%= link_to 'Destroy', microsoft, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

